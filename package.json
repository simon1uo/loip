{
  "name": "loip",
  "type": "module",
  "version": "1.0.0",
  "description": "Lorem Ipsum Text Generator for Vue 3",
  "author": "simon1uo",
  "license": "MIT",
  "keywords": [
    "vue",
    "vue3",
    "typescript",
    "component",
    "lorem",
    "ipsum",
    "lorem ipsum",
    "lipsum",
    "placeholder",
    "gibberish",
    "text",
    "random",
    "name",
    "surname",
    "full name",
    "username"
  ],
  "main": "dist/loip.umd.js",
  "module": "dist/loip.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "build:demo": "vue-tsc -b && vite build --mode demo",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.ts,.js --fix",
    "prepublishOnly": "npm run build",
    "release": "npm publish",
    "release:patch": "npm version patch && npm run release",
    "release:minor": "npm version minor && npm run release",
    "release:major": "npm version major && npm run release"
  },
  "dependencies": {
    "@vueuse/core": "^13.1.0",
    "shiki": "^3.2.2",
    "vue": "^3.5.13",
    "vue-i18n": "9"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.12.0",
    "@iconify-json/lucide": "^1.2.37",
    "@types/node": "^22.14.1",
    "@unocss/eslint-plugin": "^66.0.0",
    "@unocss/preset-attributify": "66.1.0-beta.11",
    "@unocss/preset-icons": "66.1.0-beta.11",
    "@unocss/preset-uno": "66.1.0-beta.11",
    "@unocss/reset": "66.1.0-beta.11",
    "@vitejs/plugin-vue": "^5.2.2",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.24.0",
    "typescript": "~5.7.2",
    "unocss": "66.1.0-beta.11",
    "vite": "^6.3.1",
    "vue-tsc": "^2.2.8"
  }
}
